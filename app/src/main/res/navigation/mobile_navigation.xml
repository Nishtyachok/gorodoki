<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/fragment_role_selection">


    <fragment
            android:id="@+id/competitionFragment2"
            android:name="ru.nishty.aai_referee.ui.secretary.competition_list.CompetitionFragment"
            android:label="@string/competition_list"
            tools:layout="@layout/fragment_competition_list_secretary">
        <action android:id="@+id/action_competitionFragment2_to_fragmentPerformance22"
                app:destination="@id/fragmentPerformance2"/>
        <action android:id="@+id/action_competitionFragment2_to_competitionAddFragment"
                app:destination="@id/competitionAddFragment"/>
        <action android:id="@+id/action_competitionFragment2_to_protocolQrFragment3"
                app:destination="@id/protocolQrFragment3"/>
    </fragment>
    <activity
        android:id="@+id/mainActivity"
        android:name="ru.nishty.aai_referee.MainActivity"
        android:label="activity_main"
        tools:layout="@layout/activity_main" />
    <fragment
            android:id="@+id/fragment_role_selection"
            android:name="ru.nishty.aai_referee.ui.role_selection.RoleSelectionFragment"
            android:label="@string/role_selection"
            tools:layout="@layout/fragment_role_selection">

        <action
                android:id="@+id/action_fragment_role_selection_to_competitionFragment"
                app:destination="@id/competitionFragment" />
        <action android:id="@+id/action_fragment_role_selection_to_competitionFragment2"
                app:destination="@id/competitionFragment2"/>

    </fragment>


    <fragment
        android:id="@+id/fragmentPerformance"
        android:name="ru.nishty.aai_referee.ui.referee.performance_list.PerformanceFragment"
        android:label="@string/performance_list"
        tools:layout="@layout/fragment_performance_list">

        <argument
            android:name="id"
            app:argType="string" />
        <action
            android:id="@+id/action_fragmentPerformance_to_competitionFragment"
            app:destination="@id/competitionFragment" />
        <action
            android:id="@+id/action_fragmentPerformance_to_protocolFillingFragment"
            app:destination="@id/protocolFillingFragment" />
        <argument
            android:name="discipline"
            app:argType="integer" />
        <action
            android:id="@+id/action_fragmentPerformance_to_protocolQrFragment"
            app:destination="@id/protocolQrFragment" />
    </fragment>
    <fragment
            android:id="@+id/fragmentPerformance2"
            android:name="ru.nishty.aai_referee.ui.secretary.performance_list.PerformanceFragment"
            android:label="@string/performance_list"
            tools:layout="@layout/fragment_performance_list">

        <argument
                android:name="id"
                app:argType="string"/>
        <argument
                android:name="discipline"
                app:argType="integer"/>
        <action android:id="@+id/action_fragmentPerformance2_to_protocolQrFragment2"
                app:destination="@id/protocolQrFragment2"/>
        <action android:id="@+id/action_fragmentPerformance2_to_competitionFragment22"
                app:destination="@id/competitionFragment2"/>
    </fragment>
    <fragment
        android:id="@+id/competitionFragment"
        android:name="ru.nishty.aai_referee.ui.referee.competition_list.CompetitionFragment"
        android:label="@string/competition_list"
        tools:layout="@layout/fragment_competition_list">
        <action
            android:id="@+id/action_competitionFragment_to_fragmentPerformance"
            app:destination="@id/fragmentPerformance" />

    </fragment>

    <fragment
        android:id="@+id/protocolFillingFragment"
        android:name="ru.nishty.aai_referee.ui.referee.protocol_filling.ProtocolFillingFragment"
        android:label="@string/protocol"
        tools:layout="@layout/fragment_protocol_filling">
        <argument
            android:name="protocol"
            app:argType="ru.nishty.aai_referee.entity.referee.Protocol" />
        <argument
            android:name="name"
            app:argType="string" />
        <argument
            android:name="discipline"
            app:argType="integer" />
        <action
            android:id="@+id/action_protocolFillingFragment_to_protocolQrFragment"
            app:destination="@id/protocolQrFragment" />
    </fragment>
    <fragment
        android:id="@+id/protocolQrFragment"
        android:name="ru.nishty.aai_referee.ui.referee.protocol_qr.ProtocolQrFragment"
        android:label="fragment_protocol_qr"
        tools:layout="@layout/fragment_protocol_qr" >
        <action
            android:id="@+id/action_protocolQrFragment_to_fragmentPerformance"
            app:destination="@id/fragmentPerformance"
            app:popUpTo="@id/fragmentPerformance"
            app:popUpToInclusive="true" />
        <argument
            android:name="protocol"
            app:argType="ru.nishty.aai_referee.entity.referee.Protocol" />
        <argument
            android:name="discipline"
            app:argType="integer" />
        <argument
            android:name="name"
            app:argType="string" />
    </fragment>
    <fragment
            android:id="@+id/protocolQrFragment2"
            android:name="ru.nishty.aai_referee.ui.secretary.protocol_qr.ProtocolQrFragment"
            android:label="fragment_protocol_qr"
            tools:layout="@layout/fragment_protocol_qr">
        <argument
                android:name="protocol"
                app:argType="ru.nishty.aai_referee.entity.referee.Protocol"/>
        <argument
                android:name="discipline"
                app:argType="integer"/>
        <argument
                android:name="name"
                app:argType="string"/>
        <action android:id="@+id/action_protocolQrFragment2_to_fragmentPerformance2"
                app:destination="@id/fragmentPerformance2"/>
    </fragment>
    <fragment
            android:id="@+id/protocolQrFragment3"
            android:name="ru.nishty.aai_referee.ui.secretary.protocol_qr.ProtocolQrFragment"
            android:label="fragment_protocol_qr"
            tools:layout="@layout/fragment_protocol_qr">
        <argument
                android:name="protocol"
                app:argType="ru.nishty.aai_referee.entity.referee.Protocol"/>
        <argument
                android:name="discipline"
                app:argType="integer"/>
        <argument
                android:name="name"
                app:argType="string"/>
        <action android:id="@+id/action_protocolQrFragment3_to_competitionFragment2"
                app:destination="@id/competitionFragment2"/>
    </fragment>
    <fragment
            android:id="@+id/competitionAddFragment"
            android:name="ru.nishty.aai_referee.ui.secretary.competition_list.CompetitionAddFragment"
            android:label="Создание соревнования"
            tools:layout="@layout/fragment_add_competition" >
        <action android:id="@+id/action_competitionAddFragment_to_playersFragment"
                app:destination="@id/playersFragment"/>
        <action android:id="@+id/action_competitionAddFragment_to_judgesFragment"
                app:destination="@id/judgesFragment"/>
        <action android:id="@+id/action_competitionAddFragment_to_protocolQrFragment3"
                app:destination="@id/protocolQrFragment3"/>
    </fragment>
    <fragment
            android:id="@+id/judgesFragment"
            android:name="ru.nishty.aai_referee.ui.secretary.judge_list.JudgeFragment"
            android:label="Судьи"
            tools:layout="@layout/fragment_judges" />

    <fragment
            android:id="@+id/playersFragment"
            android:name="ru.nishty.aai_referee.ui.secretary.players_list.PlayersFragment"
            android:label="Игроки"
            tools:layout="@layout/fragment_players">
        <action
                android:id="@+id/action_playersFragment_to_categoriesFragment"
                app:destination="@id/categoryFragment" />
        <action
                android:id="@+id/action_playersFragment_to_regionsFragment"
                app:destination="@id/regionFragment" />
        <action
                android:id="@+id/action_playersFragment_to_playerListFragment"
                app:destination="@id/playerListFragment" />
    </fragment>

    <fragment
            android:id="@+id/categoryFragment"
            android:name="ru.nishty.aai_referee.ui.secretary.players_list.category_list.CategoryFragment"
            android:label="Категории"
            tools:layout="@layout/fragment_category" />

    <fragment
            android:id="@+id/regionFragment"
            android:name="ru.nishty.aai_referee.ui.secretary.players_list.region_list.RegionFragment"
            android:label="Регионы"
            tools:layout="@layout/fragment_region" />

    <fragment
            android:id="@+id/playerListFragment"
            android:name="ru.nishty.aai_referee.ui.secretary.players_list.player_list.PlayerFragment"
            android:label="Список игроков"
            tools:layout="@layout/fragment_player" />


</navigation>